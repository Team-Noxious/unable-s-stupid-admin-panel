	if func then
		setfenv(func, sandbox) -- Set the environment to the sandbox
		local success, result = pcall(func) -- Execute the code safely
		if success then
			print("Code executed successfully:", result)
		else
			warn("Error during execution:", result)
		end
	else
		warn("Compilation error:", errorMessage)
	end
end)

