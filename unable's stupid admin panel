end

-- Example usage:
newscriptbutton2("archangel", function()
	print("Immortality Lord script executed!")
end)

newscriptbutton2("c00lgui", function()
	require(6032524768).Eagle(unable["local player"].Name)
end)

newscriptbutton2("666", function()
	print("Script 3 executed!")
end)

newscriptbutton2("holy wrench", function()
	require(8596757978).load(unable["local player"].Name)
end)

newscriptbutton2("disco", function()
	-- Settings
	local interval = 0.2 -- Time in seconds between color changes
	local brightnessRange = {1, 3} -- Brightness range
	local colorChangeSpeed = 5 -- Speed of color changes

	-- Function to generate a random color
	local function getRandomColor()
		return Color3.new(math.random(), math.random(), math.random())
	end

	local lighting = game:GetService("Lighting")

	while true do
		-- Randomize lighting properties
		lighting.Ambient = getRandomColor()
		lighting.OutdoorAmbient = getRandomColor()
		lighting.ColorShift_Top = getRandomColor()
		lighting.Brightness = math.random(brightnessRange[1] * 100, brightnessRange[2] * 100) / 100

		-- Wait for the next change
		task.wait(interval)
	end

end)

newscriptbutton2("k00pgui", function()
	require(14412108127).gui(unable["local player"].Name)
end)

newscriptbutton2("wallwalk", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/infyiff/backup/main/wallwalker.lua"))()
end)

newscriptbutton2("remote spy", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/realredz/SimpleSpy/refs/heads/main/Mobile.lua"))()
end)

newscriptbutton2("unanchor map", function()
	for _, descendant in pairs(workspace:GetDescendants()) do
		if descendant:IsA("BasePart") then
			descendant.Anchored = false
		end
	end
end)

